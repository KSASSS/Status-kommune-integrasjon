          DTS:ObjectName="Les SSB_04683 C Barnehager - grunnlagsdata"
          DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2017; © 2017 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
          DTS:ThreadHint="1">
          <DTS:Variables />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{87175433-C4F6-4E6D-9D6E-61FE32283CE9}"
              SQLTask:SqlStatementSource="-- Variabels&#xA;Declare @TableVar varchar(5)&#xA;Declare @TableName varchar(150)&#xA;&#xA;-- Query variabels&#xA;Declare @Truncate_Query nvarchar(200)&#xA;&#xA;Declare @antAar int&#xA;&#xA;Set @TableVar = '04683'&#xA;Set @TableName = (SELECT table_name FROM information_schema.tables WHERE table_name LIKE'SSB_' + @TableVar + '%')&#xA;&#xA;Set @Truncate_Query = N'Truncate Table [' + @TableName + ']'&#xA;&#xA;-- Vil kun ha 10 verdier totalt mellom denne og verdiene fra 12272, hvor denne står for verdier pre-2015&#xA;-- 15. mars kommer tall fra fjoråret som betyr at man vil ha ett mindre år å hente fra fra denne datoen&#xA;-- Formelen blir 8 - nåværende år - 2019 etter 15.03 og 8 - nåværende år - 2020 (som blir det samme som forrige +1) mellom 01.01 og 15.03&#xA;Set @antAar = CASE &#xA;&#x9;&#x9;&#x9;&#x9;When MONTH(GETDATE()) &gt; 3  OR (MONTH(GETDATE()) = 3 AND  DAY(GETDATE()) &gt;= 15) Then 8 - (YEAR(GETDATE()) - 2019)&#xA;&#x9;&#x9;&#x9;&#x9;Else 8 - (YEAR(GETDATE()) - 2020)&#xA;&#x9;END&#xA;&#xA;-- Prøv å hente data og sett dette inn i variabler&#xA;-- Bruker SSB_Values_AlleAar for å hente data fra SSB og SSB_Metadata for å hente metadata&#xA;begin try&#xA;&#x9;-- Declares&#xA;&#x9;declare @t table(Kommunenummer varchar(50), Variabel varchar(50), År Int, Verdi Numeric(18,4))&#xA;&#xA;&#x9;declare @m table(Kommunenavn varchar(150), Kommunenummer varchar(50), Variabelnavn varchar(150), Variabel varchar(50), År Int, År2 Int)&#xA;&#x9;&#xA;&#x9;&#xA;&#x9;-- Inserts&#xA;&#x9;insert @t Exec SSB_Values_AlleAar @TabellNummer=@TableVar, @AntallAar=@antAar&#xA;&#x9;&#xA;&#x9;insert @m Exec SSB_Metadata @TabellNummer=@TableVar&#xA;end try&#xA;&#xA;-- Noe gikk galt ved henting av data. Koden som følger skal derfor ikke eksekveres&#xA;begin catch&#xA;&#x9;RAISERROR('Did not find values in the table %s',18, 1, @TableVar)&#xA;&#x9;set noexec on&#xA;end catch&#xA;&#xA;&#xA;-- Executes&#xA;Exec sp_executesql @Truncate_Query&#xA;&#xA;Insert Into [SSB_04683 C. Barnehager - grunnlagsdata]&#xA;&#xA;Select dat.Kommunenummer,&#xA;&#x9;dat.Variabel,&#xA;&#x9;met.Variabelnavn,&#xA;&#x9;dat.År, &#xA;&#x9;dat.Verdi&#xA;&#xA;from @t dat&#xA;Join @m met ON met.Variabel = dat.Variabel&#xA;&#xA;-- Barn 1-5 år med barnehageplass, fra språklige og kulturelle minoriteter&#xA;-- Innvandrerbefolkning 1-5 år ekskl. utvalgte land&#xA;Where dat.Variabel IN ('CRC3564758912', 'CRC2024482327')&#xA;&#xA;set noexec off" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
        <DTS:Executable
          DTS:refId="Package\Mangfold og inkludering\Les SSB_12272 Minoritetsspråklige barn i barnehage 1-5 år"
